<a href="/artist-search-results">Artists found</a>
<div class="artists-cards">

    {{#each artistsList}}
    <div class="card" style="width: 18rem;">
        <img src="{{images.[0].url}}" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <p class="card-text"></p>
            <a href="/albums/{{id}}" class="btn btn-primary">view albums</a>
        </div>
    </div>
    {{/each}}
</div>




